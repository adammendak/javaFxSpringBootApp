<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="449.0" prefWidth="831.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.adammendak.crosswords.controller.TableController">
   <header>
      <AnchorPane prefHeight="50.0" prefWidth="831.0">
         <children>
            <Label fx:id="userTextLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="222.0" layoutY="12.0" prefHeight="27.0" prefWidth="408.0" text="Hasła">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </header>
   <content>
      <AnchorPane prefHeight="394.0" prefWidth="831.0">
         <children>
            <TableView fx:id="crossTable" layoutX="6.0" prefHeight="350.0" prefWidth="823.0">
              <columns>
                <TableColumn fx:id="crossNumber" prefWidth="36.0" text="#" />
                <TableColumn fx:id="crossValue" prefWidth="242.0" text="Hasło" />
                <TableColumn fx:id="crossDescription" prefWidth="542.0" text="Opis" />
              </columns>
            </TableView>
            <Button fx:id="cancelButton" layoutX="763.0" layoutY="364.0" mnemonicParsing="false" onAction="#cancel" styleClass="cancelButton" text="Wstecz" />
         </children>
      </AnchorPane>
   </content>
</DialogPane>
